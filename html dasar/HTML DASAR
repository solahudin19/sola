    <img src= "gambar/tes.jpeg" width="800px" height="500px" alt="ini foto di ocean blue">
    <br>
    <table>
        <tr>
            <td>Baris 1, kolom 1</td>
            <td>Baris 2, kolom 2</td>
        </tr>
    </table>
</body>
</html>
<table>
    <thead>
        <tr>
            <th></th>
            <th>Kolom 1</th>
            <th>Kolom 2</th>
            </tr>
            </thead>
            <tbody>
                <tr>
                    <th>Baris 1</th>
                    <td>Baris 1, kolom 1</td>
                    <td>Baris 1, kolom 2</td>
                    </tr>
                    <tr>
                        <th>Baris 2</th>
                        <td>Baris 2, Kolom 1</td>
                        <td>Baris 2, Kolom 2</td>
                        </tr>
</tbody>
<!DOCTYPE html>
<html>
    <head>
        <title>Table</title>
        </head>
        <body>

<table border="1" cellspacing="0" cellpanding="10">
    <tr>
        <td>Baris 1, Kolom 1</td>
        <td>Baris 1, Kolom 2</td>
        </tr>
        <tr>
            <td>Baris 2, kolom 1</td>
            <td>Baris 2, Kolom 2</td>
            </tr>
            </table>
<!DOCTYPE html>
            <html>
                </head>
                <title>Table</title>
            </head>
            <body>

                <table border="1" cellspacing="0" cellpanding="5">
                    <tr>
                        <td>1,1</td>
                        <td colspan="2">contoh aja</td>
                        </tr>
                        <tr>
                            <td>2,1</td>
                            <td>2,2</td>
                            <td>2,3</td>
                            </tr>
                            <tr>
                                <td>3,1</td>
                                <td>3,2</td>
                                <td>3,3</td>
                                </tr>
                            </table>

                                </body>
                                </html>
<!DOCTYPE html>
<html>
    <head>
        <title>table</title>
        </head>
        <body>

            <table border="1" cellspacing="0" cellpadding="5">
                <tr>
                    <td rowspan="3">coba coba</td>
                    <td colspan="2">contoh cuy</td>
                    </tr>
                    <tr>
                        <td>2,2</td>
                        <td>2,3</td>
                        </tr>
                        <tr>
                            <td>3,2</td>
                            <td>3,3</td>
                            </tr>
                            </table>

                            </body>
                            </html>